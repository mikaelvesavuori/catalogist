{
  "name": "catalogist",
  "version": "1.0.0",
  "description": "Catalogist is the easy way to catalog and make your software and (micro)services visible to your organization in a lightweight and developer-friendly way.",
  "homepage": "https://github.com/mikaelvesavuori/catalogist",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikaelvesavuori/catalogist.git"
  },
  "author": "Mikael Vesavuori",
  "keywords": [
    "service-catalog",
    "software-catalog",
    "service-landscape",
    "software-landscape",
    "software-observability",
    "observability",
    "discovery",
    "discovery-service",
    "discoverability",
    "software-discovery"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "npx sls offline",
    "deploy": "npx sls deploy",
    "remove": "npx sls remove",
    "lint": "npx eslint './src/**/*.ts' --quiet --fix",
    "docs": "npx arkit && npx typedoc --entryPointStrategy expand",
    "build": "npx sls package",
    "build:hosting": "npm run docs && cp -r diagrams typedoc-docs",
    "test": "npm run test:unit",
    "test:unit": "npx jest tests --collectCoverage",
    "test:unit:watch": "npx jest --clearCache && npx jest tests --collectCoverage --watch",
    "test:integration": "npx ts-node tests/integration/index.ts",
    "licenses": "npx license-compliance --direct --allow 'MIT;ISC;0BSD;BSD-2-Clause;BSD-3-Clause;Apache-2.0;Unlicense;CC0-1.0'",
    "licenses:checker": "npx license-compatibility-checker",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/aws-lambda": "8",
    "@types/jest": "27",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "arkit": "1",
    "babel-loader": "8",
    "eslint": "8",
    "eslint-config-prettier": "8",
    "eslint-plugin-prettier": "4",
    "husky": "7",
    "jest": "27",
    "license-compatibility-checker": "latest",
    "license-compliance": "latest",
    "serverless": "2",
    "serverless-iam-roles-per-function": "3",
    "serverless-offline": "8",
    "serverless-webpack": "5",
    "ts-jest": "27",
    "ts-loader": "9",
    "ts-node": "latest",
    "tslib": "latest",
    "typedoc": "latest",
    "typescript": "4",
    "webpack": "5",
    "webpack-cli": "latest"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3"
  }
}
